            {
              "key": "screenshots",
              "type": "string",
              "required": false,
              "array": true
            },
            {
              "key": "version",
              "type": "string",
              "required": false,
              "array": false
            },
            {
              "key": "compatibility",
              "type": "string",
              "required": false,
              "array": false
            },
            {
              "key": "isPublic",
              "type": "boolean",
              "required": true,
              "array": false,
              "default": false
            },
            {
              "key": "isApproved",
              "type": "boolean",
              "required": true,
              "array": false,
              "default": false
